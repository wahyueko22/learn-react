{
    "rules": {
        //"semi": ["error", "always"],
        "no-var": "error",
        //"no-console": "error",
        "no-unused-vars": "error",
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off",
        "react/prop-types": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },

    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "globals": {
        "document": false
    }
}
